{
  "name": "yhtml5-test",
  "version": "1.1.2",
  "license": "MIT",
  "main": "lib/index.js",
  "description": "A test framework for front-end projects",
  "author": "yhtml5",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "yhtml5-test --env=jsdom",
    "test:c": "npm test -- --coverage",
    "test:u": "npm i @2dfire/2dfire-scripts@latest -D",
    "git": "clear && git add . && git commit -m",
    "postgit": "git merge origin/master && git pull && git push && git status",
    "presync": "cp -rf ~/projects/gitlab/2dfire-scripts/packages/2dfire-scripts ~/projects/gits/yhtml5-cli/packages/",
    "sync": "cp -rf ~/projects/gitlab/2dfire-scripts/packages/2dfire-test/ ~/projects/gits/yhtml5-cli/packages/yhtml5-test/",
    "prepublishOnly": "echo copy... && npm version patch && npm run sync && echo npm login https://registry.npmjs.org && npm login"
  },
  "dependencies": {
    "babel-jest": "^21.0.2",
    "babel-preset-react-app": "^3.0.2",
    "cross-spawn": "^5.1.0",
    "jest": "^21.1.0",
    "leakage": "^0.3.0",
    "object-assign": "^4.1.1",
    "promise": "^8.0.1",
    "regenerator-runtime": "^0.11.0",
    "whatwg-fetch": "^2.0.3",
    "yhtml5-dev-utils": "^1.0.4"
  },
  "bin": {
    "yhtml5-test": "./bin/yhtml5-test.js"
  },
  "keywords": [
    "react",
    "cli",
    "spa"
  ],
  "engines": {
    "node": ">=4.0.0"
  }
}
